generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model guests {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  active    Boolean   @default(true)
  archive   Boolean   @default(false)
  creator   String?   @db.ObjectId
  matricule String
  password  String
  username  String

  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

type TpeProcessedCollaboratorsAttestationRib {
  banque String
  rib    String
}

type TpeProcessedCollaboratorsDiplome {
  annee      String
  diplome    String
  universite String
}

type TpeProcessedCollaboratorsFicheAntr {
  intitule String
}

type TpeProcessedCollaboratorsFicheRadio {
  ficheRadio String
}

type TpeProcessedCollaboratorsUserInfos {
  adresse         String
  birth_place     String
  birthday        DateTime? @db.Date
  city            String
  cni             String
  expiration_date DateTime? @db.Date
  firstname       String
  gender          String?
  lastname        String
  nationality     String
  ref             String
}

type TpeProcessedInfosCompany {
  /// Field referred in an index, but found no data to define the type.
  id_fiscale Json?
}

model bankchecks {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  v         Int      @map("__v")
  agent     String   @db.ObjectId
